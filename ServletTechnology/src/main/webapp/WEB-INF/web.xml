<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>ServletTechnology</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>default.html</welcome-file>
  </welcome-file-list>
  
  <servlet>
  	<servlet-name>firsts</servlet-name>
  	<servlet-class>com.demo.servlet.FirstServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>firsts</servlet-name>
  	<url-pattern>/fs</url-pattern>
  </servlet-mapping>
  
  <servlet>
  		<servlet-name>helloservlet</servlet-name>
  		<servlet-class>com.demo.servlet.HelloServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  		<servlet-name>helloservlet</servlet-name>
  		<url-pattern>/hellos</url-pattern>
  </servlet-mapping>
  
  <servlet>
  		<servlet-name>myservlet</servlet-name>
  		<servlet-class>com.demo.servlet.MyServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  		<servlet-name>myservlet</servlet-name>
  		<url-pattern>/myser</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
  		<servlet-name>lifecycle</servlet-name>
  		<servlet-class>com.demo.servlet.LifeCycleMethodDemo</servlet-class>
  </servlet>
  
  <servlet-mapping>
  		<servlet-name>lifecycle</servlet-name>
  		<url-pattern>/lifecm</url-pattern>
  </servlet-mapping>
  
  
   <servlet>
  		<servlet-name>demoser</servlet-name>
  		<servlet-class>com.demo.servlet.DemoServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  		<servlet-name>demoser</servlet-name>
  		<url-pattern>/demos</url-pattern>
  </servlet-mapping>
  
  
   <servlet>
  		<servlet-name>servlectcollabration</servlet-name>
  		<servlet-class>com.demo.servlet.ServletCollabrationDemo1</servlet-class>
  </servlet>
  
  <servlet-mapping>
  		<servlet-name>servlectcollabration</servlet-name>
  		<url-pattern>/servletc</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
  		<servlet-name>displayservlet</servlet-name>
  		<servlet-class>com.demo.servlet.DisplayServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  		<servlet-name>displayservlet</servlet-name>
  		<url-pattern>/displays</url-pattern>
  </servlet-mapping>
</web-app>